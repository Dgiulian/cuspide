{
  "kind": "collectionType",
  "collectionName": "listings",
  "info": {
    "singularName": "listing",
    "pluralName": "listings",
    "displayName": "Listing"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "property": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::property.property",
      "inversedBy": "listing"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "agente": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::agente.agente"
    },
    "currency": {
      "type": "enumeration",
      "enum": [
        "USD",
        "ARS"
      ]
    },
    "price": {
      "type": "integer"
    },
    "listing_type": {
      "type": "enumeration",
      "enum": [
        "Alquiler",
        "Venta"
      ]
    }
  }
}
